{
  "extract": {
               "url": "https://data.cityofchicago.org/resource/85ca-t3if.json",
               "fields": [ 
                           "crash_record_id",
                           "crash_date",
                           "latitude",
                           "longitude"
                         ],
               "limit": 10000 
             },
  "transform": {
                 "compute_dimensions": {
                                         "parallel_slices": 10
                                       },
                 "clustering": {
                                 "cluster_count": 8
                               }
               }
}               
